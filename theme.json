{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#383a59",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#f2f2f2",
					"name": "Text",
					"slug": "text"
				},
				{
					"color": "#bd93f9",
					"name": "Highlight 1",
					"slug": "highlight-1"
				},
				{
					"color": "#ff79c6",
					"name": "Highlight 2",
					"slug": "highlight-2"
				},
				{
					"color": "#50fa7b",
					"name": "Highlight 3",
					"slug": "highlight-3"
				},
				{
					"color": "#ffb86c",
					"name": "Highlight 4",
					"slug": "highlight-4"
				},
				{
					"color": "#8be9fd",
					"name": "Highlight 5",
					"slug": "highlight-5"
				},
				{
					"color": "#bcc2cd",
					"name": "Shadow Light",
					"slug": "shadow-light"
				},
				{
					"color": "#282a36",
					"name": "Shadow",
					"slug": "shadow"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Mulish",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/mulish/mulish-regular.woff2"
							]
						},
						{
							"fontFamily": "Mulish",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/mulish/mulish-italic.woff2"
							]
						}
					],
					"fontFamily": "\"Mulish\", sans-serif",
					"name": "Mulish",
					"slug": "mulish"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Ovo",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/ovo/ovo-regular.woff2"
							]
						}
					],
					"fontFamily": "\"Ovo\", serif",
					"name": "Ovo",
					"slug": "ovo"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--shadow)"
				}
			},
			"core/heading": {
				"elements": {
					"h1": {
						"color": {
							"text": "#50fa7b"
						}
					},
					"h2": {
						"color": {
							"text": "#8be9fd"
						}
					},
					"h3": {
						"color": {
							"text": "#bd93f9"
						}
					},
					"h4": {
						"color": {
							"text": "#bd93f9"
						}
					},
					"h5": {
						"color": {
							"text": "#bd93f9"
						}
					},
					"h6": {
						"color": {
							"text": "#bd93f9"
						}
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--text)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--background)",
					"text": "var(--wp--preset--color--highlight-2)"
				}
			},
			"h1": {
				"color": {
					"text": "#50fa7b"
				}
			},
			"h2": {
				"color": {
					"text": "#8be9fd"
				}
			},
			"h3": {
				"color": {
					"text": "#bd93f9"
				}
			},
			"h4": {
				"color": {
					"text": "#bd93f9"
				}
			},
			"h5": {
				"color": {
					"text": "#bd93f9"
				}
			},
			"h6": {
				"color": {
					"text": "#bd93f9"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ovo)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--highlight-3)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--highlight-2)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--mulish)"
		}
	}
}